{
    "database": {
        "schema": "treeherder",
        "username": "activedata2",
        "ssl": {
            "ca": "https://s3.amazonaws.com/rds-downloads/rds-combined-ca-bundle.pem"
        },
        "$ref": "~/private.json#treeherder",
        "debug": false
    },
    "analysis": {
        "limit": 1000,
        "local_db": {
            "filename": "perf_summary.sqlite",
            "debug": true
        },
        "signatures_sql": {
            "$concat": [
                "SELECT s.id",
                "FROM performance_signature s",
                "WHERE",
                "s.framework_id = 10 AND",
                "(s.test IS NULL or s.test='' or s.test=s.suite) AND",
                "s.repository_id <> 1  -- mozilla-central",
                "GROUP BY s.id"
            ],
            "separator": "\n"
        }
    },
    "constants": {
        "pyLibrary.env.http.default_headers": {
            "Referer": "MySQL-to-S3"
        },
        "mysql_to_s3.snowflake_schema.DEBUG": true
    },
    "debug": {
        "trace": true,
        "cprofile": false
    }
}


